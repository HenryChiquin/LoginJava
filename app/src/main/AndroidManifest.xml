<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginjava">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginJava">

        <activity
            android:name=".Activity_galeria"
            android:exported="false"/>
        <activity
            android:name=".Activity_Camera"
            android:exported="false"/>
        <activity
            android:name=".Activity_registro"
            android:exported="false"/>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />

        <activity
            android:name=".Activity_googlemaps"
            android:exported="false"/>
        <activity
            android:name=".Activity_planetas"
            android:exported="false"/>
        <activity
            android:name=".Activity_principalpantalla"
            android:exported="false"/>
        <activity
            android:name=".AuthActivity"
            android:exported="true" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>